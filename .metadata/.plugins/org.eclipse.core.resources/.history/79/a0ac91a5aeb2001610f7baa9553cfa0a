package com.ciadainformatica.vendas.domain;

import javax.persistence.Column;
import javax.persistence.Entity;

@Entity
public class Crediario extends GenericDomain{
	
	@Column(nullable = false)
	Pessoa pessoa;
	
	@Column(nullable = false)
	Venda venda;
	
	@Column(nullable = false)
	String valorETotal;

}
